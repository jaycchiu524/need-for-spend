{
  "name": "server",
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "start": "node dist/src/app.js",
    "npm:debug": "DEBUG=* node dist/src/app.js",
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'DEBUG=* ts-node -r tsconfig-paths/register src/app.ts'",
    "debug": "yarn build && npm run npm:debug",
    "build": "yarn prisma:generate && yarn pnpify tsc && tsc-alias && yarn run postbuild",
    "postbuild": "yarn run copy-files",
    "copy-files": "copyfiles -u 1 \"./src/generated/**/*\" dist/src",
    "docker:compose": "docker-compose --env-file=.env up -d",
    "prisma:generate": "pnpify prisma generate",
    "prisma:migrate": "pnpify prisma migrate dev",
    "prisma:push": "pnpify prisma db push",
    "prepare": "yarn run build",
    "test": "TEST=* jest -i --detectOpenHandles"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@yarnpkg/pnpify": "^4.0.0-rc.32",
    "argon2": "^0.30.2",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "express-winston": "^4.2.0",
    "helmet": "^6.0.0",
    "is-port-reachable": "^3.0.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "^3.0.0",
    "plaid": "^12.1.0",
    "typescript": "^4.9.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/is-ci": "^3.0.0",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "copyfiles": "^2.4.1",
    "docker-compose": "^0.23.17",
    "is-ci": "^3.0.1",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.1",
    "tsconfig-paths": "^4.1.2"
  }
}