{
  "private": true,
  "name": "server",
  "packageManager": "yarn@3.3.0",
  "version": "0.0.1",
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "scripts": {
    "start": "node ./dist/src/app.js",
    "build": "yarn prisma generate && yarn tsc -p tsconfig.build.json && tsc-alias && yarn post-build",
    "post-build": "copyfiles -u 1 \"./src/generated/**/*\" dist/src",
    "dev": "prisma migrate dev | nodemon --inspect=0.0.0.0:9229 --watch 'src/**' --ext 'ts,json' --ignore 'src/**/*.spec.ts' --exec 'DEBUG=* ts-node -r tsconfig-paths/register src/app.ts'",
    "db-reset": "prisma migrate reset",
    "test": "TEST=* jest"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@swc/core": "^1.3.38",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/is-ci": "^3.0.0",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.9",
    "argon2": "^0.30.2",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "docker-compose": "^0.23.17",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "helmet": "^6.0.0",
    "is-ci": "^3.0.1",
    "is-port-reachable": "^3.0.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "^3.0.0",
    "plaid": "^12.1.0",
    "prisma": "^4.7.1",
    "socket.io": "^4.6.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.3",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.3",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/supertest": "^2.0.12",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3"
  }
}